  "query": "\n        SELECT case_id, court_id, arrest_date, indictment_date,\n               arraignment_date,\n               COALESCE(trial_start_deadline, NOW() + INTERVAL '70 days') as \"trial_start_deadline!\",\n               days_elapsed, days_remaining, is_tolled, waived\n        FROM speedy_trial\n        WHERE case_id = $1 AND court_id = $2\n        ",
