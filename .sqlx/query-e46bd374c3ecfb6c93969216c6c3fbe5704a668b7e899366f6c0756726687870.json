  "db_name": "PostgreSQL",
  "query": "\n        SELECT id, court_id, case_id, name, victim_type,\n               notification_email, notification_mail, notification_phone,\n               created_at, updated_at\n        FROM victims\n        WHERE court_id = $1\n          AND ($2::TEXT IS NULL OR LOWER(name) LIKE $2)\n        ORDER BY name ASC\n        LIMIT $3 OFFSET $4\n        ",
