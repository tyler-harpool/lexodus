{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT id, court_id, case_id, filing_type, filed_by, filed_date,\n               status, validation_errors, document_id, docket_entry_id, created_at\n        FROM filings\n        WHERE court_id = $1\n          AND ($2::TEXT IS NULL\n               OR LOWER(filing_type) LIKE $2\n               OR LOWER(filed_by) LIKE $2)\n        ORDER BY filed_date DESC\n        LIMIT $3 OFFSET $4\n        ",
