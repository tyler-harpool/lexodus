{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT id, court_id, case_id, title, rule_code, due_at, status, notes, created_at, updated_at\n        FROM deadlines\n        WHERE court_id = $1\n          AND status = 'open'\n          AND due_at >= NOW()\n          AND due_at <= NOW() + INTERVAL '30 days'\n        ORDER BY due_at ASC\n        ",
